    // Place your key bindings in this file to overwrite the defaults
    [
        // End a running background task
        {
            "key": "shift+cmd+x",
            "command": "workbench.action.tasks.terminate"
        },
        // Show Source Control
        {
            "key": "shift+cmd+g",
            "command": "workbench.view.scm"
        },
        {
            "key": "ctrl+shift+g",
            "command": "-workbench.view.scm"
        },
        // Run unit tests
        {
            "key": "shift+cmd+t",
            "command": "workbench.action.tasks.test"
        }
    ]
